<Window
	x:Class="Coco_Z2.Window3" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:local="clr-namespace:Coco_Z2"
	Title="CocoZ2"
	Topmost="True"
	WindowStartupLocation="CenterScreen"
	WindowStyle="None"
	ResizeMode="NoResize"
	AllowsTransparency="True"
	BorderThickness="0"
	Background="#00FFFFFF"
	Height="400"
	Width="670"
	Loaded="CocoZ2_Loaded">
	<Grid>
		<Ellipse
			Name="CocoCircle"
			HorizontalAlignment="Center"
			Height="140"
			Margin="0, 0, 0, 260"
			VerticalAlignment="Center"
			Width="140">
			<UIElement.Effect>
				<DropShadowEffect
					ShadowDepth="0" />
			</UIElement.Effect>
			<Shape.Fill>
				<LinearGradientBrush
					EndPoint="0.5,1"
					StartPoint="0.5,0">
					<GradientStop
						Color="#FF00FE2D"
						Offset="0" />
					<GradientStop
						Color="#FF00AB64"
						Offset="1" />
				</LinearGradientBrush>
			</Shape.Fill>
		</Ellipse>
		<Image
			Name="CocoBigSur"
			HorizontalAlignment="Center"
			Height="140"
			Margin="0, 0, 0, 260"
			VerticalAlignment="Center"
			Width="140"
			Source="CocoVeryTransparentGradient.png" />
	</Grid>
</Window>
